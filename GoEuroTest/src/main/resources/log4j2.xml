<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
    <Console name="Console">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level [%thread][%logger{0}] %m%n"/>
    </Console>
    <File name="logFile" fileName="goEuroTest.log">
        <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level [%thread][%logger{0}] %m%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="Debug">
      <AppenderRef ref="logFile"/>
    </Root>
  </Loggers>
</Configuration>